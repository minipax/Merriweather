@model DateTime?
@Html.TextBox("", string.Format("{0:d}", Model))
    <script  type="text/javascript">
        $(document).ready(function () {
            $('#@ViewData.TemplateInfo.GetFullHtmlFieldId("")').datepicker({
                changeMonth: true,
                changeYear: true,
                dateFormat: 'yy-mm-dd'
            });
        });
</script>  